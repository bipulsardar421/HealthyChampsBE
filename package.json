{
  "name": "healthychampsapi",
  "version": "1.0.7",
  "description": "Mind Champs Api",
  "main": "build/main.js",
  "scripts": {
    "patch": "git stash && npm version patch && git stash pop",
    "pre-prod": "node mindchamps-api.js",
    "build": "webpack --env APP_ENV=local --mode=development --progress",
    "build:dev": "webpack --env APP_ENV=development --mode=development  --progress",
    "build:prod": "webpack --env APP_ENV=production --env=production --mode=production --progress",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:fix": "prettier --parser typescript --write \"src/**/*.ts\"",
    "start:prod": "SET NODE_ENV APP_ENV=production &&  nodemon -V --exec ts-node src/main.ts",
    "start-dev": "SET NODE_ENV APP_ENV=development &&  nodemon -V --exec ts-node src/main.ts",
    "start-wsl": "export NODE_ENV APP_ENV=development &&  nodemon -V --exec ts-node src/main.ts",
    "restart": "pm2 restart healthychampsapi --update-env",
    "start": "node pm2.js"
  },
  "author": "Mind Champs",
  "license": "ISC",
